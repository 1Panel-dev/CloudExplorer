{
  "eslint.enable": true,
  "stylelint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.validate": [
    "javascript",
    "vue",
    "html",
    "typescript"
  ],
  "stylelint.validate": [
    "css",
    "scss",
    "vue",
    "html"
  ],
  "liveServer.settings.port": 5503
}
